# pirsingsalon
$pirsyaz = {
	cls
	minut += 30
	money -= 5000
	pirsA = 1
	pirs_pain_ton = 7
	gs'stat'

	'<center><img src="images/pic/pirsing.jpg"></center>'
	'Sitting on a chair, you stuck out his tongue, Master your tongue pierced and inserted to the post.'

	act 'Escape':gt $curloc
}

$pirsyazout = {
	cls
	minut += 30
	money -= 5000
	pirsA = 0
	gs'stat'

	'<center><img src="images/pic/pirsing.jpg"></center>'
	'Master took the rod out of your language.'

	act 'Escape':gt $curloc
}

