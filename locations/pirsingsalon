gs'stat'

FColor = RGB(0,0,0)
BColor = RGB(255,255,255)
LColor = RGB(106,90,205)

'<center><b>Piercing parlor</b></center>'
'<center><img src="images/pic/pirsingstudia.jpg"></center>'
'A small salon where he works a single master.'

if hour >= 12 and hour < 20 and pirsA = 0 and money >= 5000:'<a href="exec:dynamic $pirsyaz">Make a pierced tongue 5000 rubles</a>'
if hour >= 12 and hour < 20 and pirsA = 1 and money >= 5000:'<a href="exec:dynamic $pirsyazout">Remove tongue piercing 5000 rubles</a>'
if hour >= 12 and hour < 20 and pirsB = 0 and money >= 6000:'<a href="exec:dynamic $pirsgub">Make lip piercing 6000 rubles</a>'
if hour >= 12 and hour < 20 and pirsB = 1 and money >= 6000:'<a href="exec:dynamic $pirsgubout">Remove lip piercing 6000 rubles</a>'
if hour >= 12 and hour < 20 and pirsC = 0 and money >= 3000:'<a href="exec:dynamic $pirsysh">Make pierced ears 3000 rubles</a>'
if hour >= 12 and hour < 20 and pirsC = 1 and money >= 3000:'<a href="exec:dynamic $pirsyshout">Remove pierced ears 3000 rubles</a>'
if hour >= 12 and hour < 20 and pirsD = 0 and money >= 5000:'<a href="exec:dynamic $pirsnos">Make nose piercing 5000 rubles</a>'
if hour >= 12 and hour < 20 and pirsD = 1 and money >= 5000:'<a href="exec:dynamic $pirsnosout">Remove nose piercing 5000 rubles</a>'
if hour >= 12 and hour < 20 and pirsE = 0 and money >= 5000:'<a href="exec:dynamic $pirsbro">Make eyebrow piercing 5000 rubles</a>'
if hour >= 12 and hour < 20 and pirsE = 1 and money >= 5000:'<a href="exec:dynamic $pirsbroout">Remove eyebrow piercing 5000 rubles</a>'
if hour >= 12 and hour < 20 and pirsF = 0 and money >= 10000:'<a href="exec:dynamic $pirssos">Make nipple piercings 10000 rubles</a>'
if hour >= 12 and hour < 20 and pirsF = 1 and money >= 10000:'<a href="exec:dynamic $pirssosout">Remove nipple piercings 10000 rubles</a>'
if hour >= 12 and hour < 20 and pirsN = 0 and money >= 10000:'<a href="exec:dynamic $pirspyp">Make a navel piercing 10000 rubles</a>'
if hour >= 12 and hour < 20 and pirsN = 1 and money >= 10000:'<a href="exec:dynamic $pirspypout">Remove navel piercing 10000 rubles</a>'
if hour >= 12 and hour < 20 and pirsG = 0 and money >= 25000:'<a href="exec:dynamic $pirsvla">Make piercing vagina 25000 rubles</a>'
