	healthV = vitalV*10 + strenV*5
	mannaV = intelV*magikV + magikV*100 + vitalV*10
	willpowerV = willV*10
	BonusAtakV = 0
	BonusDefV = 0
	!Action Programme
	program = 6

	test = 1
	tobiPic = 1

	unmaterialV = 40
	izanagiOnce = 0
	tobizanagi = 0
	izanagiV = 1
	GedoMazo = 0
	TobiStena = 0
