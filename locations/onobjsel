$din_cheat_work = {
	if cheatWork = 0:
		cheatWork = 1
		dynamic $din_cheat_menu_permanent
		exit
	elseif cheatWork = 1:
		cheatWork = 0
		dynamic $din_cheat_menu_permanent
	end
}

