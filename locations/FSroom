# FSroom
minut += 1
gs'stat'
gs'FSstat'

'<center><b>Street</b></center>'

act 'Go into the room':
	cls
	FShour += 1
	minut += 1
	gs'stat'

	!'You left the room.'

	gt'FSgame'
end
act 'Wander':
	cls
	FShour += 1
	minut += 1
	gs'stat'

	'You wander through the deserted streets.'

	act 'Further':gt $curloc
end
