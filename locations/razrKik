# razrKik
if razradKik = 0:
	if amatReit >= 5:
		razradKik = 1
		amatReit = 0
		kikmoney = 200

		'You have assigned 3th rank amateur kickboxing.'
	end
elseif razradKik = 1:
	if amatReit >= 10:
		razradKik = 2
		amatReit = 0
		kikmoney = 500

		'You have assigned 2th rank amateur kickboxing.'
	end
elseif razradKik = 2:
	if amatReit >= 15:
		razradKik = 3
		amatReit = 0
		kikmoney = 1500

		'You have assigned 1th rank amateur kickboxing.'
	end
elseif razradKik = 3:
	if amatReit >= 20:
		razradKik = 4
		amatReit = 0
		kikmoney = 3500

		'You awarded the title of Candidate Master of Sports in kickboxing.'
	end
elseif razradKik = 4:
	if amatReit >= 25:
		razradKik = 5
		amatReit = 0
		kikmoney = 7500

		'You awarded the title of Master of Sports in kickboxing.'
	end
end
