		'You offer 300 RUB. for this thing'

		act 'To rent':
			profi[numdres] = 0
			money += 300
			gt'loker2','start'
		end
	elseif profiH[numdres] <= 30 and profiH[numdres] > 0:
		'You offer 100 RUB. for this thing'

		act 'To rent':
			profi[numdres] = 0
			money += 100
			gt'loker2','start'
		end
	elseif profiH[numdres] <= 0:
		profi[numdres] = 0
		profiH[numdres] = 0

		'This item is worn and not suitable for further wear.'
	end
end

