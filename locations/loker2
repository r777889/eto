if $ARGS[0] = 'jeans':
	cla
	'<center><img src="images/ode/jeans<<numdres>>.jpg"></center>'
	'(Remaining strength <<jeansH[numdres]>>, The size of the hips <<jeansB[numdres]>>)'

	act 'To Top':gt'loker2','start'

	if jeansH[numdres] > 45:
		'You offer 500 RUB. for this thing'

		act 'To rent':
			jeans[numdres] = 0
			money += 500
			gt'loker2','start'
		end
	elseif jeansH[numdres] <= 45 and jeansH[numdres] > 30:
		'You offer 300 RUB. for this thing'

		act 'To rent':
			jeans[numdres] = 0
			money += 300
			gt'loker2','start'
		end
	elseif jeansH[numdres] <= 30 and jeansH[numdres] > 0:
		'You offer 100 RUB. for this thing'

		act 'To rent':
			jeans[numdres] = 0
			money += 100
			gt'loker2','start'
		end
	elseif jeansH[numdres] <= 0:
		jeans[numdres] = 0
		jeansH[numdres] = 0

		'This item is worn and not suitable for further wear.'
	end
end

