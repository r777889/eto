		if etaj5_cherdak_rnd <= 4:'You hear strange noises in the attic, and remembered the boys talking. Saying, that often someone in the attic-then fuck: <<$etaj5_cherdak_rnd>>'
		if etaj5_cherdak_rnd > 4:'<<$etaj5_cherdak_rnd>>'
		
	act 'Going down to the fourth floor': gt'pod_ezd','etaj_4'
	act 'Take the elevator': gt'pod_ezd','lift'
end
if $ARGS[0] = 'lift':
		cls
		minut +=1
		gs'stat'
		'<center><img src="images/zsoft/pod_ezd/elevator.jpg"></center>'
		'<center>An old elevator. Often broken. Hanging Ads: <a href="exec:GT ''pod_ezd'',''elev_objava''">read</a></center>'
	act 'Push button: floor 1': gt'pod_ezd','etaj_1'
	act 'Push button: floor 2': gt'pod_ezd','etaj_2'
	act 'Push button: floor 3': gt'pod_ezd','etaj_3'
	act 'Push button: floor 4': gt'pod_ezd','etaj_4'
	act 'Push button: floor 5': gt'pod_ezd','etaj_5'
pod_lift_ev = RAND(0,100)
	if pod_lift_ev <=5: dynamic $pod_lift_ev1
	if pod_lift_ev >5 and pod_lift_ev <=10: dynamic $pod_lift_ev2
	if liftnotwork_count <=0: dynamic $pod_lift_ev1
end

$pod_lift_ev1 = {
cls
minut +=1
liftnotwork_day = daystart
liftnotwork_count -= 1
gs'stat'
'<center><img src="images/zsoft/pod_ezd/liftnotwork.jpg"></center>'
'Weighs Ads: Dear zheltsy not worry during the day will master all the mend, Lift will work tomorrow!'
act 'Escape':gt'pod_ezd','etaj_2'
}
$pod_lift_ev2 = {
