if money >= 100:
	act 'Buy small goods at 100 rubles':
		cls
		minut = minut + 15
		gs'stat'

		'You went to the office and spoke with the manager about buying small things.'

		if tovarskladlevel = 0:
			if tovarL >= 30:
				'After thinking a bit, you realize, you just have nowhere to store the goods and decided to withdraw from the purchase.'
			elseif tovarL < 30:
				maxtov = 30-tovarL

				'Do you have space for storage <<maxtov>> units sold.'

				act 'Buy':
					cls
					gs'stat'

					'Do you have space for storage <<maxtov>> units sold.'

					tovarpokup = input("How much product you want to buy at a price 100 rubles apiece?")
					if tovarpokup > maxtov or tovarpokup*100 > money:
						'The manager shook his head "No, so it will not work"'
					else
						tovarL += tovarpokup
						money -= tovarpokup*100
						gs'stat'

						'You bought <<tovarpokup>> units of small goods and now you have stored <<tovarL>> units sold.'
					end

					act 'Escape':minut += 5 & gt $curloc
				end
			end
		end

		act 'Escape':minut += 5 & gt'nord'
	end

